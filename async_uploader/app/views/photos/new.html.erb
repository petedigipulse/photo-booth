<h1>List Of Photos</h1>
	<ul class="row" id='photos-list'>
		<%=render @photos %>
	</ul>

	<%=form_for @photo do |f| %>
	<div class="form-group">
		<%= f.label :author %>
		<%= f.text_field :author, class: 'form-control' %>
	</div>

	<div class="form-group">
		<%= f.label :image %>
		<%= f.file_field :author, class: 'form-control' %>
	</div> 

	<div class="form-group">
		<%= f.submit 'Submit', class: 'btn btn-primary btn-lg' %>
	</div>
	
<% end %>