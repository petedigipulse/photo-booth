<h1>List of photos</h1>

<ul class="row" id="photos-list">
	<%= render @photos %>
</ul>

<h3>Upload multiple photos</h3>
<%= form_for @photo do |f| %>
	<div class="form-group">
		<%= f.label :author %>
		<%= f.text_field :author, class: 'form-control' %>
	</div>

	<div class="form-group">
		<%= f.label :image %>
		<%= f.file :image, required: true %>
	</div>
<% end %>